{
    "name": "qlimix/process-manager",
    "type": "library",
    "description": "Process manager",
    "license": "MIT",
    "keywords": [
        "process",
        "manager"
    ],
    "authors": [
        {
            "name": "frank",
            "email": "frank@qlimix.net"
        }
    ],
    "require": {
        "php": ">7.4",
        "qlimix/process": "^2.0",
        "qlimix/time": "^2.0"
    },
    "require-dev": {
        "qlimix/code-standard": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Qlimix\\Process\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Qlimix\\Tests\\Process\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
